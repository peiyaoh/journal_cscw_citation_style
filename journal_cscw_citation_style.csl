    <category citation-format="author-date"/>
    <category field="science"/>
    <category field="social_science"/>
    <category field="generic-base"/>
    <summary> This style corresponds to 'JCSCW Instructions for Authors 2016'</summary>
    <updated>2017-03-29T00:26:36+00:00</updated>
