    <link href="https://www.zotero.org/styles/springer-humanities-author-date" rel="template"/>
    <!-- This style corresponds to 'JCSCW Instructions for Authors 2016' in the pdf document at this url -->
    <link href="http://www.springer.com/cda/content/document/cda_downloaddocument/JCSCW+Instructions+for+Authors+2016.pdf?SGWID=0-0-45-1560271-p35755499" rel="documentation"/>
    <author>
      <name>Pei-Yao Hung</name>
      <email>peiyaoh@gmail.com</email>
      <uri>http://twitter.com/peiyaoh</uri>
